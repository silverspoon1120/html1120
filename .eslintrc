{
  "extends": "eslint:recommended",
  "env": {
    "node": true
  },
  "globals": {
    "describe": true,
    "it": true
  },
  "rules": {
    "eqeqeq": 2,
    "no-extend-native": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false,
        "classes": false
      }
    ],
    "no-caller": 2,
    "no-irregular-whitespace": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-eq-null": 2,
    "no-proto": 2,
    "new-cap": 2,
    "dot-notation": 2,
    "wrap-iife": 2,
    "no-empty": 2,
    "no-with": 2,
    "no-multi-str": 2,
    "consistent-this": [
      2,
      "_this"
    ],
  }
}
